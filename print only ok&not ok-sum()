use strict;
use warnings;

use FindBin qw($Bin);
use lib "$Bin/../lib";
use MyTools;

if (sum(1,1) == 2) {
    print "ok\n";
} esle {
    print "not ok\n";
}
if (sum(2,2) == 4) {
    print "ok\n";
} esle {
    print "not ok\n";
}
if (sum(2,2,2) == 6) {
    print "ok\n";
} esle {
    print "not ok\n";
}
